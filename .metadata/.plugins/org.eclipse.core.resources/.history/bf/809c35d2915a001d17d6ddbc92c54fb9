#include "fsm_automatic.h"
#include "main.h"
#include "timer.h"

void fsm_automatic_run()
{
	switch(status)
	{
	case INIT:
		status=AUTO_RED;
		settimer1(500);
		break;
	case AUTO_RED:
		HAL_GPIO_WritePin(, GPIO_Pin, PinState)
		if(timer1_flag==1)
		{
			HAL_GPIO_
		}
		break;
	case AUTO_YELLOW:
			break;
	case AUTO_GREEN:
			break;
	default: break;
	}
}


